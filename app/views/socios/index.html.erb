<p id="notice"><%= notice %></p>

<h1>Soci</h1>

<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Surname</th>
      <th>Contact</th>
      <th>Cf</th>
      <th>Number</th>
      <th>Complete</th>
      <th>Data di registrazione</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @socios.each do |socio| %>
      <tr>
        <td><%= socio.name %></td>
        <td><%= socio.surname %></td>
        <td><%= socio.contact %></td>
        <td><%= socio.cf %></td>
        <td><%= socio.number %></td>
        <td><%= socio.complete %></td>
        <td><%= socio.registration_date %></td>
        <td><%= link_to 'Modifica', edit_socio_path(socio) %></td>
        <td><%= link_to 'Cancella', socio, method: :delete, data: { confirm: 'Sei sicuro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Aggiungi Socio', new_socio_path %>
